                android:layout_marginEnd="2dp"
                android:layout_marginBottom="0dp"
                android:backgroundTint="#8BC34A"
                android:gravity="center_horizontal"
                android:padding="15dp"
                android:paddingLeft="2dp"
                android:paddingRight="2dp"
                android:text="@string/button_text"
                android:textSize="30dp"
                app:cornerRadius="0dp"
                app:iconSize="30dp"
                app:layout_column="0"
                app:layout_row="5"
                tools:ignore="GridLayout" />

            <LinearLayout
                android:id="@+id/mybutton4"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:background="#DEB273"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?attr/selectableItemBackground"
                android:orientation="vertical"
                android:padding="0dp"
                app:layout_column="0"
                app:layout_row="1">

                <TextView
                    android:id="@+id/leftButton4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:gravity="right"
                    android:text="31"
                    android:textColor="@color/Purple2"
                    android:textSize="35dp" />

                <TextView
                    android:id="@+id/rightButton4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:text="event"
                    android:textColor="@color/Purple2"
                    android:textSize="17dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/calendar_12"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:background="#DEB273"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?attr/selectableItemBackground"
                android:orientation="vertical"
                android:padding="0dp"
                app:layout_column="1"
                app:layout_row="0">

                <TextView
                    android:id="@+id/day_12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:gravity="right"
                    android:text="31"
                    android:textColor="@color/Purple2"
                    android:textSize="35dp" />

                <TextView
                    android:id="@+id/event_12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:text="event"
                    android:textColor="@color/Purple2"
                    android:textSize="17dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <Button
                android:id="@+id/button5"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="0dp"
                android:backgroundTint="#8BC34A"
                android:gravity="center_horizontal"
                android:padding="15dp"
                android:paddingLeft="2dp"
                android:paddingRight="2dp"
                android:text="@string/button_text"
                android:textSize="30dp"
                app:cornerRadius="0dp"
                app:iconSize="30dp"
                app:layout_column="2"
                tools:ignore="GridLayout" />

            <Button
                android:id="@+id/button6"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="0dp"
                android:backgroundTint="#8BC34A"
                android:gravity="center_horizontal"
                android:padding="15dp"
                android:paddingLeft="2dp"
                android:paddingRight="2dp"
                android:text="@string/button_text"
                android:textSize="30dp"
                app:cornerRadius="0dp"
                app:iconSize="30dp"
                app:layout_column="3"
                app:layout_row="0"
                tools:ignore="GridLayout" />

            <Button
                android:id="@+id/button7"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="0dp"
                android:backgroundTint="#8BC34A"
                android:gravity="center_horizontal"
                android:padding="15dp"
                android:paddingLeft="2dp"
                android:paddingRight="2dp"
                android:text="@string/button_text"
                android:textSize="30dp"
                app:cornerRadius="0dp"
                app:iconSize="30dp"
                app:layout_column="4"
                app:layout_row="0"
                tools:ignore="GridLayout" />

            <LinearLayout
                android:id="@+id/mybutton3"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="2dp"
                android:background="#8E6427"

                android:clickable="true"
                android:focusable="true"
                android:foreground="?attr/selectableItemBackground"
                android:orientation="vertical"
                android:padding="0dp"
                android:paddingLeft="2dp"
                android:paddingRight="2dp"
                app:layout_column="5"
                app:layout_row="0">

                <TextView
                    android:id="@+id/leftButton3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:gravity="right"
                    android:text="31"
                    android:textColor="#ffffff"
                    android:textSize="35dp" />

                <TextView
                    android:id="@+id/rightButton3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:text="event"
                    android:textColor="#ffffff"
                    android:textSize="17dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/mybutton2"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:background="#8E6427"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?attr/selectableItemBackground"
                android:orientation="vertical"
                android:padding="0dp"
                app:layout_column="6"
                app:layout_row="0">

                <TextView
                    android:id="@+id/leftButton2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:gravity="right"
                    android:text="31"
                    android:textColor="#ffffff"
                    android:textSize="35dp" />

                <TextView
                    android:id="@+id/rightButton2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_gravity="center"
                    android:text="event"
                    android:textColor="#ffffff"
                    android:textSize="17dp"
                    android:textStyle="bold" />
            </LinearLayout>


        </androidx.gridlayout.widget.GridLayout>
    </LinearLayout>

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Button" />

</LinearLayout>